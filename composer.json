{
  "name": "meloncut/laravel-ali-log",
  "type": "library",
  "description": "async ALi log service collection",
  "license": "MIT",
  "keywords": ["ali-log"],
  "authors": [
    {
      "name": "Melon Charles",
      "email": "meloncut@outlook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "5.*",
    "monolog/monolog": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Meloncut\\AliLog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Meloncut\\AliLog\\Tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ]
}
